@import 'bourbon/bourbon';
@import 'grid-settings';
@import 'neat/neat';
@import '_reset';
@import '_variables';
@import '_mixins';
@import '_fonts';

html, body {
	overflow-x: hidden;
}
body {
    // background: #351e52;
    // background: -moz-linear-gradient(left,  #351e52 0%, #6e227c 50%, #682477 100%);
    // background: -webkit-gradient(linear, left top, right top, color-stop(0%,#351e52), color-stop(50%,#6e227c), color-stop(100%,#682477));
    // background: -webkit-linear-gradient(left,  #351e52 0%,#6e227c 50%,#682477 100%);
    // background: -o-linear-gradient(left,  #351e52 0%,#6e227c 50%,#682477 100%);
    // background: -ms-linear-gradient(left,  #351e52 0%,#6e227c 50%,#682477 100%);
    // background: linear-gradient(to right,  #351e52 0%,#6e227c 50%,#682477 100%);
    // filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#351e52', endColorstr='#682477',GradientType=1 );
		background-size: contain;
		background-position: center bottom;
		background-repeat: no-repeat;
		background-image: url('../img/ending-bg.png');
    background: white;
		background-size: cover;
    color: $purple;
    font-family: 'dual400', Arial, Helvetica, sans-serif;
    font-size: 16px;
    line-height: 20px;
}

.top-left-bg,
.bottom-right-bg {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1;
}
.bottom-right-bg {
    top: auto;
    left: auto;
    bottom: 0;
    right: 0;
}

img {
    max-width: 100%;
    height: auto;
}

/* Intro */
h1 {
    line-height: 42px;
    font-size: 36px;
    font-weight: normal;
    margin-bottom: 20px;

    strong {
        font-weight: normal;
        text-transform: uppercase;
    }
}

p {
    margin-bottom: 10px;
}

.nav {
    &.hide {
        display: none;
    }
}

.logo {
    position: fixed;
    right: 20px;
    top: 20px;
    z-index: 1;
}

.left {
    float: left;
}
.right {
    float: right;
}
.full {
    // margin-top: 30px;

		@include align-items(stretch);
		@include display(flex);
		@include flex-wrap(wrap);
		@include flex-direction(row);
		@include justify-content(space-between);


		@include media($large-screen-up) {
		  min-height: 66vh;
			padding-bottom: 44px;
		}
}
.half {
	@include media($large-screen-up) {
		width: 45%;
	}
}
.center {
    text-align: center;
}

.content, .intro {
    clear: both;
    overflow: hidden;
    margin: 0 auto;
}

.intro {
    color: $darkgrey;
		display: none;
    padding: 20px 0;

		p {
			margin: 0;
		}

		@include media($large-screen-up) {
		  display: block;
		}

    .purple {
        color: $purple;
    }
}
.spacer {
    height: 170px;
		display: none;
}

.start-now {
    border: 1px solid $white;
    cursor: pointer;
    display: inline-block;
    font-size: 24px;
    padding: 10px 18px;
    text-transform: uppercase;
    margin-top: 30px;

    img {
        margin-left: 10px;
        position: relative;
        top: -2px;
        vertical-align: middle;
    }
}

#tabintro {
    color: $white;
    padding-top: 80px;
		height: 100vh;
		background: #351e52;
		background: -moz-linear-gradient(left,  #351e52 0%, #6e227c 50%, #682477 100%);
		background: -webkit-gradient(linear, left top, right top, color-stop(0%,#351e52), color-stop(50%,#6e227c), color-stop(100%,#682477));
		background: -webkit-linear-gradient(left,  #351e52 0%,#6e227c 50%,#682477 100%);
		background: -o-linear-gradient(left,  #351e52 0%,#6e227c 50%,#682477 100%);
		background: -ms-linear-gradient(left,  #351e52 0%,#6e227c 50%,#682477 100%);
		background: linear-gradient(to right,  #351e52 0%,#6e227c 50%,#682477 100%);
    .content {
        width: 70%;
    }
}

.tab-content {

    .tab-pane {
        display: none;

        &.active {
            display: block;
        }

        &.white {
            .header {
                background: url("../img/header_bg.jpg") no-repeat scroll 0 0 $purple;
								background-size: cover;
                color: $white;
                font-size: 30px;
                line-height: 30px;
                padding: 0 7%;

                text-transform: uppercase;
                @include box-sizing(border-box);


                .content {
									@include align-items(center);
									@include display(flex);
									@include flex-direction(row);
									@include justify-content(flex-start);
                    background: url("../img/brocade-logo.png") no-repeat scroll right center transparent;
										width: 100%;
										max-height: 20vh;

										img {

										}
                }
            }
            .inner {
                background: $white;
                clear: both;
                overflow: hidden;
                position: relative;
                @include box-sizing(border-box);

								@include media($large-screen-up) {
									padding: 0 7% 2.5vh;
								}

                .button-previous, .button-next, .button-last {
										background: url("../img/little-arrow-left.png") no-repeat scroll 7px center / 18px transparentize(white, 0.05);
										position: fixed;
										right: 50%;
										width: 50%;
										bottom: 44px;
										padding: 16px;
										height: 50px;
										z-index: 999;
										text-transform: uppercase;
										font-family: 'dual600';
										cursor: pointer;
										padding-left: 32px;
										@include media($large-screen-up) {
											padding-top: 60px;
											margin-top:-60px;
											padding-left: 0px;
											position: absolute;
											right: 94%;
											top: 50%;
											text-align: center;
											width: 44px;
										}
                }
                .button-next,.button-last {
									background: url("../img/little-arrow-right.png") no-repeat scroll calc(100% - 7px) center / 18px transparentize(white, 0.05);
										right: 0%;
										text-align: right;
										padding-right: 32px;
										@include media($large-screen-up) {
										  margin-top:-60px;
											left: 93.5%;
											padding-right: 0px;
											right: auto;
											width: 44px;
											position: absolute;
											padding-top: 60px;
										}
                }

                .pod {
                    // background: url("../img/radial-grad.jpg") no-repeat scroll center center $lightgrey;
										@include linear-gradient(darken($midgrey,5%), $lightgrey);
										background-size: auto 200%;
										background-position: 0 100%;
                    // float: left;
                    // margin-right: 0.5%;
                    text-align: center;
                    overflow: hidden;
                    padding: 20px;
                    position: relative;

										border: 10px solid white;
										border-top: 0;
										@include align-items(center);
										@include display(flex);
										@include flex-direction(column);
										@include justify-content(center);
                    @include box-sizing(border-box);
                    @include transition(background-position .3s ease-in-out);
										@include flex-basis(100%);
										@include flex-grow(1);

										&:last-child {
											margin-bottom: 90px;
										}

										@include media($large-screen-up) {
											@include flex-basis(25%);
	                    @include flex-grow(1);
											border-left: 0;
											border-right: 10px solid white;
											border-bottom: none;
											margin-bottom: 0!important;
											&:last-child {
												border-right: none;
											}
										}


										&:nth-child(3) {
											&:before {
												content: "(a)";
												margin-bottom: 1.5em;
												font-size: 1.6em;
											}
										}

										&:nth-child(5) {
											&:before {
												content: "(b)";
												margin-bottom: 1.5em;
												font-size: 1.6em;
											}
										}

										&:nth-child(7) {
											&:before {
												content: "(c)";
												margin-bottom: 1.5em;
												font-size: 1.6em;
											}
										}

										&:nth-child(9) {
											&:before {
												content: "(d)";
												margin-bottom: 1.5em;
												font-size: 1.6em;
											}
										}

                    h3 {
                        color: $purple;
                        font-size: 26px;
                        font-weight: 100;
                        line-height: 30px;
                        // text-transform: uppercase;
                        margin-bottom: 10px;
												@include transition(all .3s ease-in-out);
												text-shadow: none;
                    }

                    p {
												@include transition(all .3s ease-in-out);
												text-shadow: none;
                        color: $purple;
                    }

                    .purple-btn {
                        background: $purple;
                        color: $white;
                        padding: 8px 14px;
												display: inline-block;
												margin-top: 1.5em;
                        // float: right;

                        img {
                            width: 16px;
                            height: auto;
                            vertical-align: middle;
                            margin-left: 30px;
                            float: right;
                        }
                    }

                    .icon {
                        display: inline-block;
                        margin-top: 20px;
												@include transition(0.3s);
												max-width: 70%;

												img {
													max-height: 145px;
												}

                        &.left-icon {
                            float: left;
														display: none;
														position: absolute;
														// width: 33%;
														bottom: 0;
														left: 0;
														opacity: 0.1;
														padding: 10px 20px;
														img {
															// width: 100%;
														}
                        }
                        &.right-icon {
                            // float: right;
                        }
                    }
                }

                input[type="radio"] {
                    position: absolute;
                    left: -99999px;
                }


								.pod.question {
									background: #850F7F;
									padding: 40px;
									position: relative;
									border-right: none;
									border-left: none;
									border-bottom: 10px solid white;

									@include flex-basis(100%);
									@include flex-grow(8);

									h3, p {
											color: $white;
									}
								}



                input[type="radio"]:checked + .pod {

									.btn {
										background-color: $orange;
									}

									.icon {
											&.left-icon {
													opacity: 1;
											}
									}

                    h3, p {
                        // color: tint($purple, 20%);
												text-shadow: 1px 1px $lightergrey;
                    }
                }

                label.pod {
                    cursor: pointer;
                }

								label.error {
									$color:#87277B;
									position: absolute;
								  z-index: 999;
								  top: 0;
								  left: 0;
								  width: 100%;
								  background: $color;
								  color: white;
								  padding: 10px;
									font-family: 'dual600';
								  text-align: center;
								}

                label.pod:hover, input[type="radio"]:checked + label.pod {
									background-position: 0 0;
									// @include radial-gradient($lightgrey, $midgrey);
                    // padding: 30px 20px;
                    // position: relative;
                    // top: -10px;
										.icon {
												&.left-icon {
														opacity: 1;
												}
										}
                    h3, p {
                        // color: tint($purple, 20%);
												text-shadow: 1px 0 $lightergrey;
                    }
                }
            }
        }
    }
}


.logo-container {
	max-width: 20%;
padding-top: 10px;
margin-bottom: -10px;
}


/* Footer */
footer {
    background: $darkpurple;
    background: -moz-linear-gradient(left, $darkpurple 0%, $purple 100%);
    background: -webkit-gradient(linear, left top, right top, color-stop(0%,$darkpurple), color-stop(100%,$purple));
    background: -webkit-linear-gradient(left, $darkpurple 0%,$purple 100%);
    background: -o-linear-gradient(left, $darkpurple 0%,$purple 100%);
    background: -ms-linear-gradient(left, $darkpurple 0%,$purple 100%);
    background: linear-gradient(to right, $darkpurple 0%,$purple 100%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='$darkpurple', endColorstr='$purple',GradientType=1);
    bottom: 0;
    left: 0;
		z-index: 10;
    padding: 10px;
    position: fixed;
    width: 100%;

    .progress {
        background: $white;
        height: 24px;
				position: relative;
        margin: 0 auto;
        padding: 5px;
        overflow: hidden;
        width: 90%;

        .bar {
            background: #aaaaaa;
						position: absolute;
						border: 3px solid $white;
						top: 0;
  					left: 0;
            height: 100%;
            width: 0;
            @include box-sizing(border-box);
            @include transition(width .6s ease);

						.percentage-counter {
							position: absolute;
							right: -40px;
						}
        }
    }
}




.interstitial {
	margin-top: 44px;
	background-size: contain;
	background-position: center 65%;
	background-repeat: no-repeat;
	@include align-items(center);
	@include display(flex);
	@include flex-direction(column);
	@include justify-content(center);

	@include media($large-screen-up) {
		background-image: url('../img/big-speech-bubble.png');

		&.two {
			background-image: url('../img/big-speech-bubble-2.png');
		}

		&.three {
			background-image: url('../img/big-speech-bubble-3.png');
		}

		&.four {
			background-image: url('../img/big-speech-bubble-4.png');
		}

		&.five {
			background-image: url('../img/big-speech-bubble-5.png');
		}
	}



	p {
		font-size: 3.5vh;
		line-height: 1.2em;
		text-align: center;
		padding: 0 25%;
	}
}

#tab11 {
	padding-bottom: 0;
}

.ending {
	// background-color: $purple;
	margin-top: 44px;
	width: 100%;
	text-align: center;

	#ending-header {
		font-size: 5em;
		line-height: 1em;
	}

	#ending-message {
		font-size: 1.2em;
		max-width: 60%;
		line-height: 1.2em;
		display: inline-block;
	}
}

.call-to-action {
	margin-top: 2em;
	h3 {
		text-transform: uppercase;
		font-size: 1.2em;
	}

	a.cta-button {
		display: inline-block;
		background-color: $purple;
		color: white;
		position: relative;
		padding: 0.5em;
		text-transform: uppercase;
		text-decoration: none;
		padding-right: 32px;
		background-image: url('../img/white-arrow-right.png');
		background-repeat: no-repeat;
		background-position: calc(100% - 5px) center;
		background-size: 18px;

		&.blue {
			background-color: #213B7F;
		}
	}


}
